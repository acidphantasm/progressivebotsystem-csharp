                            <MudButton @onclick="undoNormalizeHealthChest" Variant="Variant.Filled" Color="Color.Inherit" Size="Size.Small">Undo</MudButton>
                        }
                        else if ((_allNormalizeHealthChest != 85 || _allNormalizeHealthChestEnable != true) && MainLayout._appDefaultButtonToggle)
                        {
                            <MudButton @onclick="defaultNormalizeHealthChest" Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small">Default</MudButton>
                        }
                    </MudItem>

                    <MudItem xs="2" />
                    <MudItem xs="3">
                        <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                            <MudTooltip Text="Health of Stomach. Toggle switch to enable or disable this body part." Color="Color.Primary" Placement="Placement.Right" Arrow="true">
                                <MudText>Stomach Health</MudText>
                            </MudTooltip>
                        </MudPaper>
                    </MudItem>
                    <MudItem xs="1">
                        <MudPaper Class="d-flex align-center mud-width-full" Elevation="0">
                            <MudSwitch T="bool" @bind-Value="_allNormalizeHealthStomachEnable" Color="Color.Success" UncheckedColor="Color.Default" Disabled="!_allNormalizeHealthEnable"/>
                        </MudPaper>
                    </MudItem>
                    <MudItem xs="1">
                        <MudButton Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small" Disabled="!_allNormalizeHealthStomachEnable || !_allNormalizeHealthEnable">@_allNormalizeHealthStomach</MudButton>
                    </MudItem>
                    <MudItem xs="4">
                        <MudSlider @bind-Value="_allNormalizeHealthStomach" ValueLabel="true" Min="1" Max="250" Color="Color.Info" Size="Size.Medium" Disabled="!_allNormalizeHealthStomachEnable || !_allNormalizeHealthEnable" />
                    </MudItem>
                    <MudItem xs="1">
                        @if ((MainLayout.pendingChanges.Contains("_allNormalizeHealthStomach") || MainLayout.pendingChanges.Contains("_allNormalizeHealthStomachEnable")) && MainLayout._appUndoButtonToggle)
                        {
                            <MudButton @onclick="undoNormalizeHealthStomach" Variant="Variant.Filled" Color="Color.Inherit" Size="Size.Small">Undo</MudButton>
                        }
                        else if ((_allNormalizeHealthStomach != 70 || _allNormalizeHealthStomachEnable != true) && MainLayout._appDefaultButtonToggle)
                        {
                            <MudButton @onclick="defaultNormalizeHealthStomach" Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small">Default</MudButton>
                        }
                    </MudItem>

                    <MudItem xs="2" />
                    <MudItem xs="3">
                        <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                            <MudTooltip Text="Health of Left Arm. Toggle switch to enable or disable this body part." Color="Color.Primary" Placement="Placement.Right" Arrow="true">
                                <MudText>Left Arm Health</MudText>
                            </MudTooltip>
                        </MudPaper>
                    </MudItem>
                    <MudItem xs="1">
                        <MudPaper Class="d-flex align-center mud-width-full" Elevation="0">
                            <MudSwitch T="bool" @bind-Value="_allNormalizeHealthLeftArmEnable" Color="Color.Success" UncheckedColor="Color.Default" Disabled="!_allNormalizeHealthEnable"/>
                        </MudPaper>
                    </MudItem>
                    <MudItem xs="1">
                        <MudButton Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small" Disabled="!_allNormalizeHealthLeftArmEnable || !_allNormalizeHealthEnable">@_allNormalizeHealthLeftArm</MudButton>
                    </MudItem>
                    <MudItem xs="4">
                        <MudSlider @bind-Value="_allNormalizeHealthLeftArm" ValueLabel="true" Min="1" Max="250" Color="Color.Info" Size="Size.Medium" Disabled="!_allNormalizeHealthLeftArmEnable || !_allNormalizeHealthEnable" />
                    </MudItem>
                    <MudItem xs="1">
                        @if ((MainLayout.pendingChanges.Contains("_allNormalizeHealthLeftArm") || MainLayout.pendingChanges.Contains("_allNormalizeHealthLeftArmEnable")) && MainLayout._appUndoButtonToggle)
                        {
                            <MudButton @onclick="undoNormalizeHealthLeftArm" Variant="Variant.Filled" Color="Color.Inherit" Size="Size.Small">Undo</MudButton>
                        }
                        else if ((_allNormalizeHealthLeftArm != 60 || _allNormalizeHealthLeftArmEnable != true) && MainLayout._appDefaultButtonToggle)
                        {
                            <MudButton @onclick="defaultNormalizeHealthLeftArm" Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small">Default</MudButton>
                        }
                    </MudItem>

                    <MudItem xs="2" />
                    <MudItem xs="3">
                        <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                            <MudTooltip Text="Health of Right Arm. Toggle switch to enable or disable this body part." Color="Color.Primary" Placement="Placement.Right" Arrow="true">
                                <MudText>Right Arm Health</MudText>
                            </MudTooltip>
                        </MudPaper>
                    </MudItem>
                    <MudItem xs="1">
