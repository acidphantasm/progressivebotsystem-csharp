            Utils.UpdateView(value, ModConfig.OriginalConfig.PmcBots.Enable);
            ModConfig.Config.PmcBots.Enable = value;
        }
    }
    private void undoPMCBots() => _pmcEnabled = ModConfig.OriginalConfig.PmcBots.Enable;
    private void defaultPMCBots() => _pmcEnabled = true;
