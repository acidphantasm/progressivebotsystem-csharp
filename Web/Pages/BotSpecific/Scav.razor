@code {
    #region ENABLE / DISABLE
    private bool _scavEnabled
    {
        get => ModConfig.Config.ScavBots.Enable;
        set
        {
            Utils.UpdateView(value, ModConfig.OriginalConfig.ScavBots.Enable);
            ModConfig.Config.ScavBots.Enable = value;
        }
    }
    private void undoScavBots() => _scavEnabled = ModConfig.OriginalConfig.ScavBots.Enable;
    private void defaultScavBots() => _scavEnabled = true;
    #endregion
    #region LOOT
    private bool _scavLootEnabled
    {
        get => ModConfig.Config.ScavBots.LootConfig.Enable;
        set
        {
            ModConfig.Config.ScavBots.LootConfig.Enable = value;
            Utils.UpdateView(value, ModConfig.OriginalConfig.ScavBots.LootConfig.Enable);
        }
    }
    private string _lootBlacklistEntryHolder { get; set; } = "";

    private List<string> _scavLootBlacklist
    {
        get => ModConfig.Config.ScavBots.LootConfig.Blacklist;
        set
        {
            ModConfig.Config.ScavBots.LootConfig.Blacklist = value;
            Utils.UpdateView(value, ModConfig.OriginalConfig.ScavBots.LootConfig.Blacklist);
        }
    }

    private void undoLoot()
    {
        _scavLootEnabled = ModConfig.OriginalConfig.ScavBots.LootConfig.Enable;
        _scavLootBlacklist.Clear();
        foreach (var item in ModConfig.OriginalConfig.ScavBots.LootConfig.Blacklist)
        {
            _scavLootBlacklist.Add(item);
        }
        MainLayout.pendingChanges.Remove("_scavLootBlacklist");
    }

    private void defaultLoot()
    {
        _scavLootEnabled = true;
        _scavLootBlacklist.Clear();
        MainLayout.pendingChanges.Add("_scavLootBlacklist");
    }
    #endregion
    #region REROLL
    private bool _scavWeaponAmmunitionRollEnabled
    {
        get => ModConfig.Config.ScavBots.RerollConfig.Enable;
        set
        {
            Utils.UpdateView(value, ModConfig.OriginalConfig.ScavBots.RerollConfig.Enable);
            ModConfig.Config.ScavBots.RerollConfig.Enable = value;
            ModConfig.Config.ScavBots.ToploadConfig.Enable = !value;
        }
    }
    private int _scavWeaponAmmunitionRollChance
    {
        get => ModConfig.Config.ScavBots.RerollConfig.Chance;
        set
        {
            Utils.UpdateView(value, ModConfig.OriginalConfig.ScavBots.RerollConfig.Chance);
            ModConfig.Config.ScavBots.RerollConfig.Chance = value;
        }
    }
    private void undoRollAmmunitionEnabled()
    {
        _scavWeaponAmmunitionRollEnabled = ModConfig.OriginalConfig.ScavBots.RerollConfig.Enable;
        _scavWeaponAmmunitionRollChance = ModConfig.OriginalConfig.ScavBots.RerollConfig.Chance;
        _scavWeaponToploadEnabled = ModConfig.OriginalConfig.ScavBots.ToploadConfig.Enable;
        _scavWeaponToploadChance = ModConfig.OriginalConfig.ScavBots.ToploadConfig.Chance;
        _scavWeaponToploadPercent = ModConfig.OriginalConfig.ScavBots.ToploadConfig.Percent;
    }
